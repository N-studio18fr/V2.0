  `num_posts` mediumint(8) UNSIGNED NOT NULL DEFAULT 0,
  `last_post` int(10) UNSIGNED DEFAULT NULL,
  `last_post_id` int(10) UNSIGNED DEFAULT NULL,
  `last_poster` varchar(200) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `last_topic` varchar(255) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `sort_by` tinyint(1) NOT NULL DEFAULT 0,
  `disp_position` int(10) NOT NULL DEFAULT 0,
  `cat_id` int(10) UNSIGNED NOT NULL DEFAULT 0,
  `no_sum_mess` tinyint(1) NOT NULL DEFAULT 0,
  `parent_forum_id` int(10) UNSIGNED NOT NULL DEFAULT 0
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

