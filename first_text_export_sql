CREATE TABLE `forums` (
  `id` int(10) UNSIGNED NOT NULL,
  `forum_name` varchar(80) COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT 'New forum',
  `forum_desc` text COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `redirect_url` varchar(100) COLLATE utf8mb4_unicode_ci DEFAULT NULL,
  `moderators` text COLLATE utf8mb4_unicode_ci DEFAULT NULL,
